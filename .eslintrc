{
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "amd": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "no-console": "off"
  }
}